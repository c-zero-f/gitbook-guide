{
  "title": "gitbooke使用文档-学习笔记",
  "description": "gitbook-guide",
  "language": "zh-hans",
   "plugins": [
       "splitter",
       "github",
       "prism",
       "-highlight",
       "tbfed-pagefooter",
       "expandable-chapters",
	   "anchors",
       "anchor-navigation-ex",
       "page-footer"  
    ],
  "pluginsConfig": {
    "prism": {
       "css": [
         "prismjs/themes/prism-okaidia.css"
       ]
    },
    "disqus": {
      "shortName": "gitbook-guide"
    },
    "github": {
      "url": "https://github.com/zq99299/gitbook-guide"
    },
    "baidu": {
        "token": "a9787f0ab4"
    },
    "sitemap": {
        "hostname": "http://mrcode.cn/"
    },
    "github-buttons": {
      "repo": "zq99299/gitbook-guide",
      "types": [
        "star",
        "watch"
      ],
      "size": "large"
    },
    "tbfed-pagefooter": {
        "copyright":"Copyright &copy mrcode.cn",
        "modify_label": "该文件修订时间：",
        "modify_format": "YYYY-MM-DD HH:mm:ss"
    },
    "toc": {
        "addClass": true,
        "className": "toc"
    },
    "atoc": {
        "addClass": true,
        "className": "atoc"
    },
    "expandable-chapters":{},
    "components": {
      "templatePath": "components",
      "templates": [
        {
          "name": "header",
          "target": "body",
          "prepend": "true"
        },
        {
          "name": "footer",
          "target": ".book",
          "prepend": "false"
        }
      ]
    },
    "page-treeview": {
            "style": "markdown",
            "copyright": "Copyright &#169; zq99299", 
            "minHeaderCount": "1",
            "minHeaderDeep": "1"
     },
    "page-footer": {
            "description": "modified at",
            "signature": "Aleen",
            "wisdom": "More than a coder, more than a designer",
            "format": "yyyy-MM-dd hh:mm:ss",
            "copyright": "Copyright &#169; zq99299",
            "timeColor": "#666",
            "copyrightColor": "#666",
            "utcOffset": "8",
            "isShowQRCode": true,
            "baseUri": "https://aleen42.gitbooks.io/personalwiki/content/",
            "isShowIssues": true,
            "repo": "zq99299/gitbook-guide",
            "issueNum": "8",
            "token": "",
            "style": "normal"
    },
	"anchor-navigation-ex":{
		"isRewritePageTitle":true
	}
  }
} 