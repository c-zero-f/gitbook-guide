{
  "title": "gitbooke使用文档-学习笔记",
  "description": "gitbook-guide",
  "language": "zh-hans",
   "plugins": [
       "splitter",
       "github",
       "prism",
       "-highlight",
       "anchors",
       "tbfed-pagefooter",
       "expandable-chapters",
       "navigator",
       "-page-treeview"  
    ],
  "pluginsConfig": {
    "prism": {
       "css": [
         "prismjs/themes/prism-okaidia.css"
       ]
    },
    "disqus": {
      "shortName": "gitbook-guide"
    },
    "github": {
      "url": "https://github.com/zq99299/gitbook-guide"
    },
    "baidu": {
        "token": "a9787f0ab4"
    },
    "sitemap": {
        "hostname": "http://mrcode.cn/"
    },
    "github-buttons": {
      "repo": "zq99299/gitbook-guide",
      "types": [
        "star",
        "watch"
      ],
      "size": "large"
    },
    "tbfed-pagefooter": {
        "copyright":"Copyright &copy mrcode.cn",
        "modify_label": "该文件修订时间：",
        "modify_format": "YYYY-MM-DD HH:mm:ss"
    },
    "toc": {
        "addClass": true,
        "className": "toc"
    },
    "atoc": {
        "addClass": true,
        "className": "atoc"
    },
    "expandable-chapters":{},
    "components": {
      "templatePath": "components",
      "templates": [
        {
          "name": "header",
          "target": "body",
          "prepend": "true"
        },
        {
          "name": "footer",
          "target": ".book",
          "prepend": "false"
        }
      ]
    },
    "page-treeview": {
            "style": "markdown",
            "copyright": "Copyright &#169; zq99299", 
            "minHeaderCount": "1",
            "minHeaderDeep": "1"
     }
  }
} 